<div class="content table-responsive table-full-width padding_none">
	<div class="Vertical-heading">
      <ul class="nav nav-tabs" id = "vertical_floor_nav">
        <li role="presentation" class="active navigate_goods"><a href="#">商品</a></li>
        <li role="presentation" class="navigate_ad"><a href="#">广告</a></li>
        <li role="presentation" class="navigate_nav"><a href="#">导航</a></li>
      </ul>
  <div>
</div>

<div class="content table-responsive table-full-width padding_none">
	<div class="picture_text_content">
		<article>

			<form class="form-horizontal" role="form">
			    {{#each data}}
				    <div class="form-group">
					    <label  class="col-sm-3 control-label" for="ds_host">商品sku</label>
					    <div class="col-sm-5">
							<input class="form-control sku" value = "{{sku}}" id="ds_host" type="text">
					    </div>
					</div>
					<div  class="form-group">
						    <label class="col-sm-3 control-label" for="ds_host">商品链接</label>
						    <div class="col-sm-5">
								<input class="form-control uri" value = "{{uri}}" id="ds_host" type="text">
						    </div>
						    <div class="col-sm-4">	
						    </div>
					</div>
					<div  class="form-group">
						    <label class="col-sm-3 control-label" for="ds_host">主标题</label>
						    <div class="col-sm-5">
								<input class="form-control main_title" value = "{{main_title}}" id="ds_host" type="text">
						    </div>
						    <div class="col-sm-4">	
						    </div>
					</div>
					<div  class="form-group">
						    <label class="col-sm-3 control-label" for="ds_host">副标题</label>
						    <div class="col-sm-5">
								<input class="form-control side_title" value = "{{side_title}}" id="ds_host" type="text">
						    </div>
						    <div class="col-sm-4">	
						    </div>
					</div>
					<div  class="form-group">
						    <label class="col-sm-3 control-label" for="ds_host">月供信息</label>
						    <div class="col-sm-5">
								<input class="form-control pay_month" value = "{{pay_month}}" id="ds_host" type="text">
						    </div>
						    <div class="col-sm-4">	
						    </div>
					</div>
					<div  class="form-group">
					    <label class="col-sm-3 control-label" for="ds_host">售价</label>
					    <div class="col-sm-5">
							<input class="form-control price" value = "{{price}}" id="ds_host" type="text">
					    </div>
					    <div class="col-sm-4">	
					    </div>
					</div>
					<div class="form-group">
						   <div class="col-sm-3 control-label parent-span" for="ds_host">
								<span class="span-submit">
								    <div>上传图片</div>
								</span>
						   </div>
						   <div class="col-sm-5" style="position:relative;">
						       <div class="form-control input_image">
						       	  <i class="fa fa-plus-square"></i>
						       </div>
						       <input type="file" value="上传图片" class="summit_picture1" id = "file" name = "file" />
						       <img src="{{pic_url}}" class="Upload_file1" id="ds_host" />
						   </div>
						    
						   <div class="col-sm-4">
						   </div>	
					</div>
				{{/each}}
			</form>
		</article>
	</div>
</div>


<button class="btn btn-info edit_confirm fr">确定</button>
<button class="btn btn-warning edit_quit fr">取消</button>